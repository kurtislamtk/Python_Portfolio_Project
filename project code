    "import re\n",
    "def add_space(text):\n",
    "    spaced_text = re.sub(r'(?<!^)(?=[A-Z])',' ',text)\n",
    "    return spaced_text\n",
    "\n",
    "add_space(sector.find('ul',class_='z1s6m00 z1s6m03 _5135ge0 _5135ge5').getText())"
   ]
